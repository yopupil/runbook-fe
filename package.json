{
  "name": "@nineleaps/aura",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "abortcontroller-polyfill": "^1.1.9",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codemirror": "5.46.0",
    "css-loader": "0.28.7",
    "cuid": "^2.1.6",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsdoc": "^3.6.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rangy": "^1.3.0",
    "react": "^16.8.6",
    "react-codemirror2": "^6.0.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.4.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^4.3.1",
    "react-tabs": "^3.0.0",
    "react-tooltip": "^3.6.1",
    "redux": "^4.0.1",
    "resolve": "1.6.0",
    "semistandard": "^13.0.1",
    "slug": "^1.1.0",
    "smoothscroll-polyfill": "^0.4.3",
    "snarkdown": "^1.2.2",
    "socket.io-client": "^2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "3.0.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "start": "PUBLIC_URL=http://localhost:3000/app node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "lintfix": "./node_modules/eslint/bin/eslint.js src --fix",
    "test": "node scripts/test.js --env=jsdom",
    "doc": "jsdoc -r src/**/*.js",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:8763/",
      "secure": false
    },
    "/auth/*": {
      "target": "http://localhost:8763/"
    },
    "/socket.io": {
      "target": "http://localhost:8763/",
      "ws": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupEnzymeAdapter.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/index.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "moduleDirectories": [
        "node_modules",
        "src"
      ],
      "(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^4.10.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier-standard": "^9.1.1",
    "raw-loader": "^1.0.0",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "1.1.8"
  }
}
